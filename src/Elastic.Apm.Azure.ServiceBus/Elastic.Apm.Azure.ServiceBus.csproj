<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
      <TargetFrameworks>netstandard2.0; net5.0</TargetFrameworks>
      <AssemblyName>Elastic.Apm.Azure.ServiceBus</AssemblyName>
      <RootNamespace>Elastic.Apm.Azure.ServiceBus</RootNamespace>
      <PackageId>Elastic.Apm.Azure.ServiceBus</PackageId>
      <Description>Elastic APM for Azure Service Bus. This package contains auto instrumentation for Azure.Messaging.ServiceBus 
        and Microsoft.Azure.ServiceBus packages.</Description>
      <PackageTags>apm, monitoring, elastic, elasticapm, analytics, azure, service, bus, servicebus</PackageTags>
      <IsPackable>true</IsPackable>
    </PropertyGroup>
  
    <ItemGroup>
      <InternalsVisibleTo Include="Elastic.Apm.Azure.ServiceBus.Tests" Key="$(ExposedPublicKey)" />
    </ItemGroup>
  
    <ItemGroup>
      <ProjectReference Include="..\Elastic.Apm\Elastic.Apm.csproj" />
    </ItemGroup>

</Project>

{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ElasticApm": {
    "LogLevel": "Debug",
    "ServerUrls": "http://myServerFromTheConfigFile:8080",
    "ServiceName": "My.Test.Application",
    "ServiceNodeName": "Instance1",
    "ServiceVersion": "2.1.0.5",
    "Environment": "staging",
    "CaptureHeaders": "false",
    "TransactionSampleRate": 0.456,
    "TransactionMaxSpans": 375,
    "CaptureBody": "all",
    "CaptureBodyContentTypes" :  "application/x-www-form-urlencoded*, text/*, application/json*, application/xml*"
  }
}
